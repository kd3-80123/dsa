package com.sunbeam;

public class Program01 {
	
	
	
	public static void main(String[] args) {
		
		
	
		Integer [] arr = {1,3,6,4,7,9,10};
		int key = 3;
		int count = 1;
		
		
		for(int i=0;i< arr.length;i++ )
		{
			if(key!=arr[i])
				count++;
		}
		System.out.println("Linear search no of comparisions done to search a key is "+count);
}
	
}
