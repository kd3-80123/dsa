package com.sunbeam;

public class Program04 {
	public static int occurancereturn(int [] arr,int size, int key ,int occur)
	{
		int count=0;
		for(int i=0;i<size;i++)
		{
			if(key==arr[i])
			{
				count ++;
			}
		}
		if(count==0)
		return -1;
		
		else
			return count;
	}

	public static void main(String[] args) {
	
		int [] arr = {1,4,5,2,6,3,8,9,4,5,1,2,4,6};
		
		int count = occurancereturn(arr, arr.length, 2,2);
		if(count==-1)
			System.out.println("key not found");
		else
			System.out.println("Instance of occurnace ");

	}

}
