package com.sunbeam;

public class Program02_BinarySearch {

	public static void main(String[] args) {
		Integer [] arr = {1,2,3,4,5,6,7,8,9};
		int key =5;
		int count=0;
		int left = 0,mid;
		int right = (arr.length-1);
		
		
		mid = (left+right)/2;
		if(arr[mid]==key)
			System.out.println("Found the key");
		else if(arr[mid]>key)
			right = mid-1;
		
		
		
	}

} 
