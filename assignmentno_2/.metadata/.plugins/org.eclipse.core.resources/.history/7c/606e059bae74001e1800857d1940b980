package com.sunbeam;

public class BubbleSort {
	
	public static int bubbleSort(int arr[])
	{
		int count=0;
		for (int i=0;i<arr.length;i++)
		{
			for(int j=0;j<arr.length;j++)
			{count++;
				if(arr[j]>arr[j+1])
				{
					int temp = arr[j];
					arr[j] =arr[j+1];
					arr[j+1]= arr[j];
				}
			}
		}
	}
	public static void main(String[] args) {
	

	}

}
