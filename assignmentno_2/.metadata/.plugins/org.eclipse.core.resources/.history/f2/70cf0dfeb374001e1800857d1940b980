package com.sunbeam;

public class InsertionSort {
	public static int insertionSort(int arr[]) 
	{
		int count=0;
		for(int i=1;i<arr.length;i++)
		{
			
				int temp = arr[i];
				int j =i-1;
				while(j>=0 && arr[j]>temp)
				{
					arr[i]= arr[j];
					count++;
					j--;
				}
				arr[j]=temp;		
		}
		
		return count;
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
